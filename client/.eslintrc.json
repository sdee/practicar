{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "promise",
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "no-mixed-spaces-and-tabs": ["error"],
        "no-trailing-spaces": ["error"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}]
    }
}