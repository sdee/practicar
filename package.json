{
  "name": "practicar-server",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/client/"
    ]
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node ./bin/www",
    "client": "babel-node start-client.js",
    "postinstall": "cd client && npm install && npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "webpack": "4.41.2",
    "webpack-cli": "^3.1.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "^4.17.1",
    "fast-diff": "^1.1.1",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/node": "^7.8.3",
    "concurrently": "^3.1.0",
    "jest": "^24.9.0"
  },
  "engines": {
    "node": "12.14.x"
  }
}
