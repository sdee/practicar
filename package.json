{
  "name": "practicar-server",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node ./bin/www",
    "client": "babel-node start-client.js",
    "postinstall": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "^4.17.3",
    "fast-diff": "^1.1.1",
    "morgan": "^1.9.1",
    "redux-thunk": "^2.2.0",
    "redux-undo": "^0.6.1",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "concurrently": "^3.1.0"
  },
  "engines": {
    "node": "6.9.x"
  }
}
